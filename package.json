{
  "name": "@brighthustle/adonisjs-whatsapp",
  "description": "Connect your WhatsApp Cloud API with AdonisJS",
  "version": "1.0.5-9",
  "main": "build/index.js",
  "type": "module",
  "files": [
    "build/src",
    "build/services",
    "build/providers",
    "build/stubs",
    "build/configure.js",
    "build/configure.d.ts",
    "build/index.d.ts",
    "build/index.js"
  ],
  "exports": {
    ".": "./build/index.js",
    "./services/main": "./build/services/main.js",
    "./whatsapp_provider": "./build/providers/whatsapp_provider.js",
    "./types": "./build/src/types/main.js"
  },
  "scripts": {
    "test": "c8 npm run quick:test",
    "pretest": "npm run lint",
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"stubs/**/**/*.stub\" build",
    "compile": "npm run lint && npm run clean && tsc && npm run copy:templates && npm run index:commands",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "np --message=\"chore(release): %s\"  --no-tests",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels .github/labels.json CodeSyncr/adonisjs-whatsapp",
    "index:commands": "adonis-kit index build/commands"
  },
  "devDependencies": {
    "@adonisjs/core": "^6.1.5-24",
    "@adonisjs/eslint-config": "^1.1.8",
    "@adonisjs/prettier-config": "^1.1.8",
    "@adonisjs/tsconfig": "^1.1.8",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "c8": "^8.0.0",
    "commitizen": "^4.3.0",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.44.0",
    "eslint-plugin-prettier": "^5.0.0",
    "github-label-sync": "^2.2.0",
    "husky": "^8.0.3",
    "np": "^7.6.3",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@poppinss/utils": "6.5.0-3",
    "axios": "^1.3.2",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.1.5-24"
  },
  "author": "Suluh S, Kumar Yash",
  "license": "MIT",
  "homepage": "https://github.com/CodeSyncr/adonisjs-whatsapp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeSyncr/adonisjs-whatsapp.git"
  },
  "bugs": {
    "url": "https://github.com/CodeSyncr/adonisjs-whatsapp/issues"
  },
  "keywords": [
    "whatsapp",
    "cloud-api",
    "whatsapp-api",
    "whatsapp-cloud",
    "whatsapp-cloud-api",
    "whatsapp-business",
    "whatsapp-business-api",
    "adonis",
    "adonisjs",
    "adonisjs-whatsapp"
  ],
  "types": "build/adonis-typings/index.d.ts",
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": "@adonisjs/prettier-config",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "np": {
    "message": "chore(release): %s",
    "contents": ".",
    "branch": "develop",
    "tag": "next",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**",
      "src/repl_bindings.ts"
    ]
  }
}
